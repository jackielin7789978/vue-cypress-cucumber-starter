{
  "name": "vue-bdd",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:bdd": "concurrently \"vite\" \"cypress open --e2e\"",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "concurrently \"vite preview\" \"cypress open --e2e --config baseUrl=http://localhost:4173/\"",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e --config baseUrl=http://localhost:4173/'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.13",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "cypress": "^10.7.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.0.9",
    "vite-plugin-windicss": "^1.8.8",
    "vitest": "^0.23.0",
    "vue-tsc": "^0.40.7",
    "windicss": "^3.5.6"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e/step_definitions/*.{js,ts}",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/e2e/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
